@model Trocas

@{
    ViewData["Title"] = "Create";
}

<h1>Create</h1>

<h4>Trocas</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="IdLivroDado" class="control-label"></label>
                <input asp-for="IdLivroDado" class="form-control" />
                <span asp-validation-for="IdLivroDado" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a onclick="window.history.back()" class="btn btn-outline-primary">Voltar</a>
</div>

